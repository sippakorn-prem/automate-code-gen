{
  "name": "Code Gen",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Automate code generator",
  "permissions": [
    "storage",
    "activeTab",
    "*://*/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Code A Gen",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}